{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "functions": {
    "app.py": {
      "maxDuration": 300,
      "memory": 3008
    }
  },
  "installCommand": "pip install -r requirements.txt && python setup_models.py"
}
